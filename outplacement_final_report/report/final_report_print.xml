                            </span>
                            <br/>
                            <b>Complementing information:</b>
                            <t t-if="o.complementing_information == 'komm_info_01'">
                                Information has been communicated
                            </t>
                            <t t-elif="o.complementing_information == 'komm_info_02'">
                                Information has not been communicated
                            </t>
                            <t t-elif="o.complementing_information == 'komm_info_03'">
                                Information missing
                            </t>
                        </div>
                    </t>
                </t>
            </t>
        </template>

        <report
                id="outplacement_final_report"
                model="outplacement"
                string="Final Report"
                report_type="qweb-pdf"
                name="outplacement_final_report.report_outplacement_final"
                file="outplacement_final_report.report_outplacement_final"
                print_report_name="'%s - Final Report' % (object.name)"
                groups="base.group_no_one"
        />
    </data>
</odoo>