<odoo>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">outplacement.view.form.inherit.jobs</field>
        <field name="model">outplacement</field> 
        <field name="inherit_id" ref="outplacement.outplacement_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Final report">
                    <group>
                        <field name="obstacle_id"/>
                        <field name="obstacle_reason"/>
                        <field name="obstacle_motivation"/>
                    </group>
                    <group>
                        <field name="study_visit_ids" mode="tree" nolabel="1">
                            <tree editable="bottom" string="Study visits" default_order="name">
                                <field name="name"/>
                                <field name="visit_type"/>
                                <field name="reasoning"/>
                            </tree>
                        </field>
                    </group>
                </page>
            </xpath>
            <field name="date_end" position="after">
                <field name="send_date"/>
            </field>
        </field>
    </record>
</odoo>