<odoo>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">outplacement.view.form.inherit.final.report</field>
        <field name="model">outplacement</field>
        <field name="inherit_id" ref="outplacement.outplacement_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button type="object" string="Send report" name="send_final_report"/>
            </xpath>
        </field>
    </record>
</odoo>