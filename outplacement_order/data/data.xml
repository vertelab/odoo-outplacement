<odoo>
    <data noupdate="0">
        <record id="outplacement.incoming_stage" model="outplacement.stage">
            <field name="name">Incoming</field>
            <field name="ordertjansten_status">DEFINITIV</field>
            <field name="order_id_state">sale</field>
            <field name="sequence">10</field>
        </record>
        <record id="outplacement.in-progress_stage" model="outplacement.stage">
            <field name="name">In-Progress</field>
            <field name="ordertjansten_status">PRELIMINAR</field>
            <field name="order_id_state">sent</field>
            <field name="sequence">20</field>
        </record>
        <record id="outplacement.sent_stage" model="outplacement.stage">
            <field name="fold">1</field>
            <field name="name">Sent</field>
            <field name="ordertjansten_status">KLAR, LEVERERAD</field>
            <field name="order_id_state">done</field>
            <field name="sequence">30</field>
        </record>
        <record id="outplacement.cancelled_stage" model="outplacement.stage">
            <field name="fold">1</field>
            <field name="name">Cancelled</field>
            <field name="ordertjansten_status">MAKULERAD, AVBRUTEN, AVBRUTEN_KLAR</field>
            <field name="order_id_state">cancel</field>
            <field name="sequence">40</field>
        </record>
        <record id="sale.action_orders" model="ir.actions.act_window">
            <field name="domain"></field>
        </record>
    </data>
</odoo>
