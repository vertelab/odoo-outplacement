<odoo>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">outplacement.view.form.inherit.education</field>
        <field name="model">outplacement</field> 
        <field name="inherit_id" ref="outplacement.outplacement_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="education" string="Education">
                    <group>
                        <group>
                            <field name="education_ids" mode="tree" nolabel="1">
                                <tree editable="bottom" string="Educations">
                                    <field name="sun_id"/>
                                    <field name="education_level_id"/>
                                    <field name="foreign_education"/>
                                    <field name="foreign_education_approved"/>
                                </tree>
                            </field>
                            <field name="cv_file_name" invisible="1"/>
                            <field name="cv" filename="cv_file_name"/>
                            <field name="references_file_name" invisible="1"/>
                            <field name="references" filename="references_file_name"/>
                        </group>
                        <group>
                            <field name="drivers_license_ids"
                                   widget="many2many_tags"
                                   options="{'no_create_edit': True}"/>
                            <field name="has_car"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>