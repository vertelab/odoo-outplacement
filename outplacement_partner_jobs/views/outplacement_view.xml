<odoo>
    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">outplacement.view.form.inherit.jobs</field>
        <field name="model">outplacement</field> 
        <field name="inherit_id" ref="outplacement.outplacement_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Desired jobs">
                <group>
                  <field name="job_ids" mode="tree" nolabel="1" attrs="{'invisible': [('partner_id', '=', False)]}">
                    <tree editable="bottom" string="Desired jobs" default_order="name">
                        <field name="ssyk_code"/> 
                        <field name="name"  options="{'no_create_edit': True}"/>
                        <field name="education_level"/>
                        <field name="education"/>
                        <field name="experience_length"/>
                        <field name="experience"/>
                    </tree>
                  </field>
                </group>
            </page>
            </xpath>
        </field>
    </record>
</odoo>