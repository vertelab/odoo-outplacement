<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="product_suborder" model="product.product">
        <field name="name">Product Suborder</field>
        <field name="standard_price">1.0</field>
        <field name="list_price">1.0</field>
        <field name="type">consu</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>
    
    <record forcecreate="False" id="task_stage_data_0" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">New</field>
            <field name="project_ids" eval="[(4, ref('project_project_data'))]"/>
    </record>

    <record forcecreate="False" id="task_stage_data_1" model="project.task.type">
            <field name="sequence">2</field>
            <field name="name">Ongoing</field>
            <field name="project_ids" eval="[(4, ref('project_project_data'))]"/>
    </record>

    <record forcecreate="False" id="task_stage_done" model="project.task.type">
            <field name="sequence">10</field>
            <field name="name">Done</field>
            <field name="project_ids" eval="[(4, ref('project_project_data'))]"/>
    </record>

    
</odoo>
